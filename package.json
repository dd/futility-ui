{
  "name": "futility-ui",
  "version": "0.0.0",
  "description": "A set of ready-to-use UI components for Vue 3, styled according to the Flowbite design system, providing a fast start and a consistent visual style for your application.",
  "main": "dist/futility-ui.cjs.js",
  "module": "dist/futility-ui.es.js",
  "exports": {
    ".": {
      "require": "./dist/futility-ui.cjs.js",
      "import": "./dist/futility-ui.es.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "vite build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p ${APP_PORT:-6008} --no-open",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "components",
    "ui-library",
    "flowbite",
    "design-system",
    "tailwindcss",
    "frontend",
    "component-library"
  ],
  "author": "Dmitry Dobrynin <dd@tovarisch.engineer>",
  "license": "LGPL-3.0",
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/vue3": "^8.4.7",
    "@storybook/vue3-vite": "^8.4.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "sass": "^1.82.0",
    "storybook": "^8.4.7",
    "vite": "^6.0.3"
  }
}
